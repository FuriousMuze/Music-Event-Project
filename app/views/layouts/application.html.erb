<!DOCTYPE html>
<html>
<head>
  <title>MusicEventProject</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	
	<div class="button"><button type="button" class="btn btn-default navbar-btn"><%= link_to "Register", new_user_path %></button></div>
<% if current_user  %>
  <div class="button"><button type="button" class="btn btn-default navbar-btn"><%= link_to "Logout", logout_path %></button></div>
<% else %>
	<div class="button"><button type="button" class="btn btn-default navbar-btn"><%= link_to "Login", login_path %></button></div>
<% end %>

<%= yield %>

</body>
</html>
